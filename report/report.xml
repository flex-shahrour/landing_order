        <template id="landing_order_view">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <div class="row">
                                <div class="col-xs-12">
                                    <h2>Report Title</h2>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12">
                                    <h3>Report Subtitle</h3>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12">
                                    <table class="table table-condensed">
                                        <thead>
                                            <tr>
                                                <th>Column 1</th>
                                                <th>Column 2</th>
                                                <th>Column 3</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>Value 1</td>
                                                <td>Value 2</td>
                                                <td>Value 3</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </t>
                </t>
            </t>
        </template>


        <template id="report_invoice_34">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-set="lang" t-value="o.partner_id.lang"/>
                    <!--                    <t t-if="o._get_name_invoice_report() == 'flex_landing_orders.landing_order_view'"-->
                    <!--                       t-call="flex_landing_orders.landing_order_view"-->
                    <!--                       t-lang="lang"/>-->
                </t>
            </t>
        </template>

        <template id="report_invoice_with_payments_dfdf">
            <t t-call="flex_landing_orders.report_invoice_34">
                <t t-set="landing_order_view" t-value="True"/>
            </t>
        </template>

    </data>
</odoo>