<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <report
                id="report_landing_order"
                model="landing.order"
                string="ReportTitle"
                name="landing_order_view"
                file="flex_landing_orders.report_landing_order"
                report_type="qweb-pdf"/>

